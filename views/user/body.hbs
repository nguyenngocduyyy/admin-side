<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-info">
                        <h4 class="card-title ">Users</h4>
                        <p class="card-category"> List of users</p>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead class=" text-primary">
                                    <tr>
                                        <th>
                                            Username
                                        </th>
                                        <th>
                                            Name
                                        </th>
                                        <th>
                                            Surname
                                        </th>
                                        <th>
                                            Email
                                        </th>
                                        <th>
                                            Status
                                        </th>
                                        <th>
                                            Detail
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each users}}
                                    <tr>
                                        <td>
                                            {{username}}
                                        </td>
                                        <td>
                                            {{name}}
                                        </td>
                                        <td>
                                            {{surname}}
                                        </td>
                                        <td>
                                            {{email}}
                                        </td>
                                        <td name="status" class="text-success">
                                            {{status}}
                                        </td>
                                        <td>
                                            <a href="/users/{{_id}}">
                                                <i class="material-icons">contact_page</i>
                                            </a>
                                        </td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                {{#not q}}
                                {{#not productId}}
                                <ul class="pagination justify-content-center">
                                    {{#isnt currentPage 1}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?page={{prevPage}}" aria-label="Previous">
                                            <span aria-hidden="true">&laquo;</span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                    </li>
                                    {{/isnt}}

                                    {{#if hasPreviousPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?page={{prevPage}}">{{prevPage}}</a>
                                    </li>
                                    {{/if}}

                                    <li class="page-item active">
                                        <a class="page-link" href="javascript:void(0)" tabindex="-1">{{currentPage}}</a>
                                    </li>

                                    {{#if hasNextPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?page={{nextPage}}">{{nextPage}}</a>
                                    </li>
                                    {{/if}}

                                    {{#gt lastPage currentPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?page={{nextPage}}" aria-label="Next">
                                            <span aria-hidden="true">&raquo;</span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </li>
                                    {{/gt}}
                                </ul>
                                {{/not}}
                                {{/not}}
                                {{#if productId}}
                                <ul class="pagination justify-content-center">
                                    {{#isnt currentPage 1}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?productId={{productId}}&page={{prevPage}}"
                                            aria-label="Previous">
                                            <span aria-hidden="true">&laquo;</span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                    </li>
                                    {{/isnt}}

                                    {{#if hasPreviousPage}}
                                    <li class="page-item">
                                        <a class="page-link"
                                            href="/users/?productId={{productId}}&page={{prevPage}}">{{prevPage}}</a>
                                    </li>
                                    {{/if}}

                                    <li class="page-item active">
                                        <a class="page-link" href="javascript:void(0)" tabindex="-1">{{currentPage}}</a>
                                    </li>

                                    {{#if hasNextPage}}
                                    <li class="page-item">
                                        <a class="page-link"
                                            href="/users/?productId={{productId}}&page={{nextPage}}">{{nextPage}}</a>
                                    </li>
                                    {{/if}}

                                    {{#gt lastPage currentPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?productId={{productId}}&page={{nextPage}}"
                                            aria-label="Next">
                                            <span aria-hidden="true">&raquo;</span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </li>
                                    {{/gt}}
                                </ul>
                                {{/if}}
                                {{#if q}}
                                <ul class="pagination justify-content-center">
                                    {{#isnt currentPage 1}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?q={{q}}&page={{prevPage}}" aria-label="Previous">
                                            <span aria-hidden="true">&laquo;</span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                    </li>
                                    {{/isnt}}

                                    {{#if hasPreviousPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?q={{q}}&page={{prevPage}}">{{prevPage}}</a>
                                    </li>
                                    {{/if}}

                                    <li class="page-item active">
                                        <a class="page-link" href="javascript:void(0)" tabindex="-1">{{currentPage}}</a>
                                    </li>

                                    {{#if hasNextPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?q={{q}}&page={{nextPage}}">{{nextPage}}</a>
                                    </li>
                                    {{/if}}

                                    {{#gt lastPage currentPage}}
                                    <li class="page-item">
                                        <a class="page-link" href="/users/?q={{q}}&page={{nextPage}}" aria-label="Next">
                                            <span aria-hidden="true">&raquo;</span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </li>
                                    {{/gt}}
                                </ul>
                                {{/if}}
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

</div>